@page
@using DFApp.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model DFApp.Web.Pages.Bookkeeping.Expenditure.EditModalModel;
@inject IStringLocalizer<DFAppResource> L
@{
    Layout = null;
}


<abp-dynamic-form abp-model="ExpenditureDto" asp-page="/Bookkeeping/Expenditure/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["BookkeepingExpenditure:Modal:Update:Title"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id"></abp-input>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel | AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
