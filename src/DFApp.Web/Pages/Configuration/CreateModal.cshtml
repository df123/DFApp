@page
@using DFApp.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model DFApp.Web.Pages.Configuration.CreateModalModel
@inject IStringLocalizer<DFAppResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="ConfigurationInfoViewModel" asp-page="/Configuration/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["ConfigurationInfo:Modal:Create:Title"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>