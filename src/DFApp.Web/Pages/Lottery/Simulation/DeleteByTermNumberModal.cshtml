@page
@using DFApp.Localization
@using Microsoft.Extensions.Localization
@model DFApp.Web.Pages.Lottery.Simulation.DeleteByTermNumberModalModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal;
@inject IStringLocalizer<DFAppResource> L

<form asp-page="/Lottery/Simulation/DeleteByTermNumberModal">
    <abp-modal>
        <abp-modal-header title="@L["LotterySimulation:DeleteByTermNumber"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="TermNumber" label="@L["LotterySimulation:TermNumber"].Value"/>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>
